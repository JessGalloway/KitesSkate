@model Dictionary<int, KitesSkate.UI.MVC.Models.CartItemViewModel>

@{
    ViewData["Title"] = "Cart";
}


@foreach (var item in Model)
{

    <div class="row justify-content-center">
        <div class="col-md-12 justify-content-center">
            <span>
                @item.Value.Cartprod.GearName (@item.Value.Qty)
            </span>
        </div>
    </div>
    
}